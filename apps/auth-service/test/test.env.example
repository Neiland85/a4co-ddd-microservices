# Variables de entorno para pruebas de auth-service
# Copia este archivo a .env.test y ajusta los valores según tu entorno

TEST_USERNAME=test_user
TEST_PASSWORD=test_password
TEST_EMAIL=test@example.com

# Configuración de base de datos de prueba
TEST_DB_HOST=localhost
TEST_DB_PORT=5432
TEST_DB_NAME=auth_service_test
TEST_DB_USER=test_user
TEST_DB_PASSWORD=test_password

# Configuración de JWT para pruebas
TEST_JWT_SECRET=test_jwt_secret_key_for_testing_only
TEST_JWT_EXPIRES_IN=1h

# Configuración de Redis para pruebas
TEST_REDIS_HOST=localhost
TEST_REDIS_PORT=6379
TEST_REDIS_PASSWORD=

# Configuración de logging para pruebas
TEST_LOG_LEVEL=error
TEST_LOG_FILE=test.log
