generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ProductType {
  MADE_TO_ORDER
  SEASONAL
}

model Product {
  id        String      @id @default(uuid())
  name      String
  type      ProductType
  price     Float
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
}
