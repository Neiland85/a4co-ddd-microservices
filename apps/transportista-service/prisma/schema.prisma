generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transporter {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  status    String   // e.g., "available", "busy"
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
